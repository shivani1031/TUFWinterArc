Max Sum Subarray of size K


class Solution {
  public:
    int maxSubarraySum(vector<int>& arr, int k) {
        // code here
        int n = arr.size();
        int sum=0;
        for(int i =0;i<k;i++)
        sum+=arr[i];
        int maxcurr = sum;
        for(int i =k;i<n;i++)
        {
           sum = sum+  arr[i]-arr[i-k];
           maxcurr = max(maxcurr, sum);
        }
        return maxcurr;
    }
};
