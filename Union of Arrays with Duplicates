Union of Arrays with Duplicates


class Solution {
  public:
    vector<int> findUnion(vector<int>& a, vector<int>& b) {
        // code here
        set<int> s;
        for(int i =0;i<a.size();i++)
        {
            s.insert(a[i]);
        }
        for(int j =0;j<b.size();j++)
        {
            s.insert(b[j]);
        }
        vector<int> result;
    for(auto it = s.begin(); it != s.end(); it++) {
    result.push_back(*it);
}
return result;
    }
};
