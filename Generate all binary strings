Generate all binary strings




class Solution {
  public:
  void helper(int n, string curr, vector<string> &result)
  {
  if(curr.size()==n)
  {
      result.push_back(curr);
      return;
  }
  helper(n, curr+'0', result);
 
  helper(n, curr+'1', result);
  }
  
  
    vector<string> binstr(int n) {
        // code here
        vector<string> result;
        string curr = "";
        helper(n, "", result);
        return result;
    }
};

