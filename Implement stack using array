Implement stack using array



class myStack {
  public:
  int n;
  vector<int> ans;
    myStack(int n) {
        // Define Data Structures
        this->n =n;
    }

    bool isEmpty() {
        // check if the stack is empty
        if (ans.size()==0)
        return true;
        else 
        return false;
    }

    bool isFull() {
        // check if the stack is full
        if(ans.size()==n)
        return true;
        else 
        return false;
        
    }

    void push(int x) {
        // inserts x at the top of the stack
        if(ans.size()<n)
        {
            ans.push_back(x);
        }
        
    }

void pop() {
    if (!isEmpty()) {
        ans.pop_back();
    }
}


    int peek() {
        // Returns the top element of the stack
        if (!isEmpty()) {
            return ans.back();
        }
        return -1; // or throw exception
    }
};
